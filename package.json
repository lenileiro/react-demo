{
  "name": "reactssr",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev:serve": "webpack-dev-server --env.CLIENT=development --mode development --open",
    "dev:build": "webpack --env.CLIENT=development",
    "prod:build": "webpack --env.CLIENT=build --mode production",
    "prod:serve": "npm run prod:build -w & nodemon server.js --env.CLIENT=production --mode production",
    "test": "jest"
  },
